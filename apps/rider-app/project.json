{
  "name": "rider-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/rider-app/src",
  "projectType": "application",
  "targets": {
    "analyze": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter analyze",
        "cwd": "apps/rider-app"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter clean",
        "cwd": "apps/rider-app"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter format apps/rider-app/*",
        "cwd": "apps/rider-app"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter test",
        "cwd": "apps/rider-app"
      }
    },
    "assemble": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter assemble",
        "cwd": "apps/rider-app"
      }
    },
    "attach": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter attach",
        "cwd": "apps/rider-app"
      }
    },
    "drive": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter drive",
        "cwd": "apps/rider-app"
      }
    },
    "genL10n": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter gen-l10n",
        "cwd": "apps/rider-app"
      }
    },
    "install": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter install",
        "cwd": "apps/rider-app"
      }
    },
    "run": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter run",
        "cwd": "apps/rider-app"
      }
    },
    "buildAar": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build aar",
        "cwd": "apps/rider-app"
      }
    },
    "buildApk": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build apk",
        "cwd": "apps/rider-app"
      }
    },
    "buildAppbundle": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build appbundle",
        "cwd": "apps/rider-app"
      }
    },
    "buildBundle": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build bundle",
        "cwd": "apps/rider-app"
      }
    },
    "buildIos": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build ios",
        "cwd": "apps/rider-app"
      }
    },
    "buildIosFramework": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build ios-framework",
        "cwd": "apps/rider-app"
      }
    },
    "buildIpa": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build ipa",
        "cwd": "apps/rider-app"
      }
    }
  },
  "tags": []
}
